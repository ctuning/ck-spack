{
  "auto_detect": "yes",
  "customize": {
    "add_only_with_version": "yes",
    "add_to_path": "yes",
    "build_dir_name": "spack",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_SPACK_NATIVE",
    "limit_recursion_dir_search": {
      "linux": 3
    },
    "only_for_host_os_tags": [
      "linux"
    ],
    "soft_file": {
      "linux": "spack"
    },
    "soft_file_from_host_os": "yes",
    "soft_path_example": {
      "linux": "/usr/bin/cmake"
    },
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1"
    }
  },
  "soft_name": "spack",
  "tags": [
    "spack",
    "native"
  ]
}
